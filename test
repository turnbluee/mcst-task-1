v0x5cc2045cb3d0_0 .net "reset", 0 0, v0x5cc2045cb880_0;  1 drivers
v0x5cc2045cb470_0 .var "sum", 3 0;
E_0x5cc2045b40a0 .event posedge, v0x5cc2045cb120_0;
S_0x5cc204578490 .scope module, "adder" "bit_sum" 3 13, 4 1 0, S_0x5cc2045b5a70;
 .timescale 0 0;
    .port_info 0 /INPUT 16 "in";
    .port_info 1 /OUTPUT 4 "bit_sum";
P_0x5cc2045b5d80 .param/l "DATA_W" 0 4 2, +C4<00000000000000000000000000010000>;
P_0x5cc2045b5dc0 .param/l "POS_W" 0 4 3, +C4<00000000000000000000000000000100>;
L_0x5cc2045cb950 .functor BUFZ 4, v0x5cc2045cafe0_0, C4<0000>, C4<0000>, C4<0000>;
v0x5cc204579590_0 .net "bit_sum", 3 0, L_0x5cc2045cb950;  alias, 1 drivers
v0x5cc2045cae10_0 .var/i "i", 31 0;
v0x5cc2045caef0_0 .net "in", 15 0, v0x5cc2045cb6c0_0;  alias, 1 drivers
v0x5cc2045cafe0_0 .var "sum", 3 0;
E_0x5cc2045b44c0 .event anyedge, v0x5cc2045cafe0_0, v0x5cc2045caef0_0;
    .scope S_0x5cc204578490;
T_0 ;
    %wait E_0x5cc2045b44c0;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x5cc2045cafe0_0, 0, 4;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5cc2045cae10_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5cc2045cae10_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x5cc2045cafe0_0;
    %load/vec4 v0x5cc2045caef0_0;
    %load/vec4 v0x5cc2045cae10_0;
    %part/s 1;
    %pad/u 4;
    %add;
    %store/vec4 v0x5cc2045cafe0_0, 0, 4;
    %load/vec4 v0x5cc2045cae10_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5cc2045cae10_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5cc2045b5a70;
T_1 ;
    %wait E_0x5cc2045b40a0;
    %load/vec4 v0x5cc2045cb3d0_0;
    %flag_set/vec4 8;
    %jmp/0 T_1.0, 8;
    %pushi/vec4 0, 0, 4;
    %jmp/1 T_1.1, 8;
T_1.0 ; End of true expr.
    %load/vec4 v0x5cc2045cb2d0_0;
    %jmp/0 T_1.1, 8;
 ; End of false expr.
    %blend;
T_1.1;
    %assign/vec4 v0x5cc2045cb470_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5cc2045b5810;
T_2 ;
    %delay 1, 0;
    %load/vec4 v0x5cc2045cb600_0;
    %inv;
    %store/vec4 v0x5cc2045cb600_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5cc2045b5810;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5cc2045cb600_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5cc2045cb880_0, 0;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x5cc2045cb6c0_0, 0;
    %delay 1, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5cc2045cb880_0, 0;
    %pushi/vec4 39587, 0, 16;
    %store/vec4 v0x5cc2045cb6c0_0, 0, 16;
    %delay 6, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5cc2045cb880_0, 0;
    %delay 2, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5cc2045cb880_0, 0;
    %pushi/vec4 11223, 0, 16;
    %store/vec4 v0x5cc2045cb6c0_0, 0, 16;
    %delay 6, 0;
    %vpi_call 2 39 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5cc2045b5810;
T_4 ;
